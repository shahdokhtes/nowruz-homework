using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;
using System.Threading.Tasks;

namespace ConsoleApplication15
{
    class Program
    {
        static void Main(string[] args)
        {
            double a, b, s=0;
            string r;

            a = int.Parse(Console.ReadLine());
            r = Console.ReadLine();
            b = int.Parse(Console.ReadLine());

            if (r == "*")
                s = a * b;
           
            else if (r == "/")
                s = a / b;

            else if (r == "+")
                s = a + b;

            else if (r == "-")
                s = a - b;

            Console.WriteLine("javab= "+s);

            Console.ReadLine();
        }
    }
}
